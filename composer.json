{
    "name": "diskominfotik/pwdskt",
    "description": "Diskominfotik Password Sakti Helper for Laravel",
    "keywords": [
        "diskominfotik",
        "pwdskt"
    ],
    "homepage": "https://git.jakarta.go.id/diskominfotik/pwdskt",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Abu Abbas",
            "email": "notificationfor.gitlabs@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Diskominfotik\\Pwdskt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diskominfotik\\Pwdskt\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Diskominfotik\\Pwdskt\\PwdsktServiceProvider"
            ]
        }
    }
}
